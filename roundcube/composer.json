{
  "name": "roundcube/roundcubemail",
  "description": "The Roundcube Webmail suite",
  "license": "GPL-3.0-or-later",
  "repositories": [
    {
      "type": "composer",
      "url": "https://plugins.roundcube.net"
    },
    {
      "type": "vcs",
      "url": "https://gitlab.thejohnsons.site/acjohnson/roundcube-plugin-filters.git"
    },
    {
      "type": "vcs",
      "url": "https://gitlab.thejohnsons.site/acjohnson/roundcube-plugin-advanced_search.git"
    }
  ],
  "require": {
    "php": ">=7.3.0",
    "pear/pear-core-minimal": "~1.10.11",
    "pear/auth_sasl": "~1.1.0",
    "pear/mail_mime": "~1.10.1",
    "pear/net_smtp": "~1.10.1",
    "pear/crypt_gpg": "~1.6.7",
    "pear/net_sieve": "~1.4.6",
    "roundcube/plugin-installer": "~0.3.1",
    "roundcube/rtf-html-php": "~2.1",
    "masterminds/html5": "~2.8.0",
    "johndoh/contextmenu": "^3.3",
    "bacon/bacon-qr-code": "^2.0.7",
    "guzzlehttp/guzzle": "^7.7.0",
    "acjohnson/filters": "dev-master",
    "acjohnson/advanced_search": "dev-master",
    "alexandregz/twofactor_gauthenticator": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "suggest": {
    "kolab/net_ldap3": "~1.1.4 required for connecting to LDAP",
    "bjeavons/zxcvbn-php": "^1.3 required for Zxcvbn password strength driver"
  },
  "config": {
    "allow-plugins": {
      "roundcube/plugin-installer": true
    }
  }
}
